            <key> <string>_body</string> </key>
            <value> <string encoding="cdata"><![CDATA[

portal_catalog = context.getPortalObject().portal_catalog\n
kw = {}\n
\n
if query is not None:\n
  kw[\'full_text\'] = query\n
if uid is not None:\n
  kw[\'uid\'] = uid\n
\n
page = int(page)\n
step = 5\n
offset = 5 * page\n
limit = 5 * page + 5 + 1\n
kw.update(\n
  portal_type=\'Software Release\',\n
  validation_state=\'published\',\n
  select_dict={\n
    \'title\': None,\n
    \'description\': None,\n
    \'software_release_url\': \'software_release.url\',\n
  },\n
  limit=(offset, limit),\n
  **{\'software_release.state\': \'stopped\'}\n
)\n
\n
result = [q for q in portal_catalog(**kw)]\n
if len(result) > 5:\n
  after = True\n
else:\n
  after = False\n
response = []\n
for q in result[:5]:\n
    o = q.getObject()\n
    response.append({\n
      \'uid\': q.UID,\n
      \'title\': q.TITLE,\n
      \'description\': q.DESCRIPTION,\n
      \'image_url\': context.getPermanentURL(o.getFollowUpValue(portal_type=\'Software Product\').getDefaultImageValue()) + \'/Base_download\',\n
      \'document_url\': context.getPermanentURL(o),\n
      \'url\': q.SOFTWARE_RELEASE_URL,\n
    })\n
\n
return context.asJSON({\n
  \'software_release_list\':response,\n
  \'before\': page > 0 and True or False,\n
  \'after\': after,\n
  \'page\': page,\n
  \'query\': query\n
  })\n


]]></string> </value>
